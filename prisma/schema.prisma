// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model trip {
	id Int @id @default(autoincrement())
	user Int
	name String
	start_date DateTime @default(now())
	end_date DateTime?
}